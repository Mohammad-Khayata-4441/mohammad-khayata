{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "overview": {
      "type": "richtext"
    },
    "company": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "media": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "primaryColor": {
      "type": "string"
    },
    "technologies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::technology.technology",
      "mappedBy": "project"
    },
    "skills": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::skill.skill",
      "mappedBy": "project"
    },
    "url": {
      "type": "string"
    },
    "repo": {
      "type": "string"
    },
    "experience": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::experience.experience",
      "inversedBy": "projects"
    },
    "seo": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    },
    "openGraph": {
      "type": "component",
      "component": "shared.open-graph",
      "repeatable": false
    }
  }
}
